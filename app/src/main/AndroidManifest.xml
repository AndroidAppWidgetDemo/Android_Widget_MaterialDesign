        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.xiaxl.md.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.xiaxl.md.SnackbarActivity"></activity>
        <activity android:name="com.xiaxl.md.TextInputLayoutActivity"></activity>
        <activity android:name="com.xiaxl.md.FloatingActionButtonActivity"></activity>
        <activity android:name="com.xiaxl.md.tablayout.TabLayoutActivity"></activity>
        <activity android:name="com.xiaxl.md.NavigationViewActivity"></activity>
        <activity android:name="com.xiaxl.md.coordinatorlayout.CoordinatorLayoutBehavior01Activity"></activity>
        <activity android:name="com.xiaxl.md.coordinatorlayout.CoordinatorLayoutBehavior02Activity"></activity>
        <activity android:name="com.xiaxl.md.AppBarLayoutActivity"></activity>




    </application>

</manifest>